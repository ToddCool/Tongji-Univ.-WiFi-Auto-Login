<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.Tongji.AutoLoginWiFi</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/AutoLoginWifi.sh</string>
    </array>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/Library/Logs/WiFiAutoLogin-logfile.log</string>
    <key>StandardErrorPath</key>
    <string>/Library/Logs/WiFiAutoLogin-errorlogfile.log</string>
  </dict>
</plist>
